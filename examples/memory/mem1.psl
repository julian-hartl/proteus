import "std/mem";
import "std/io";
struct Foo {
    a: Int;
    b: Int;
}

fn main() {
    val foo = &Foo{a: 1, b: 2};

    println((*foo).a);
    println((*foo).b);

    free(foo);

    println((*foo).a);
    println((*foo).b);

    var test: &Foo = malloc(5);
    test = &Foo{a: 1, b: 2};

    println((*test).a);
    println((*test).b);

}