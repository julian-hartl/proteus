fn main() {
    for x in 1 until 10 {
        var i = -1;
        println("x = " + x as String);
        if x == 5 {
            continue;
        }
        while(i < 10) {
            i = i + 1;
            if i == 5 {
                continue;
            }
            println("i = " + i as String);
        }
    }
}

main();